10 FOR I=1 TO 3
20 READ P
30 PRINT "Probing "+HEX$(P)+"h"
40 O=&B00101111
50 OUT P,O
60 A=INP(P)AND&B11111100
70 GOSUB 230
80 A1=A
90 O=&B11011011
100 OUT P,O
110 A=INP(P)AND&B11111100
120 GOSUB 230
130 A2=A
140 O=&B11110111
150 OUT P,O
160 A=INP(P)AND&B11111100
170 GOSUB 230
180 A3=A
190 IF A1=204 AND A2=136 AND A3=68 THEN PRINT"JIOCART detected at "+HEX$(P)+"h":GOTO 250
200 NEXT I
210 PRINT"JIOCART NOT FOUND!"
220 GOTO 250
230 PRINT "W:"+RIGHT$("00000000"+BIN$(O),8)+" "+HEX$(O)+STR$(O)+" R:"+RIGHT$("00000000"+BIN$(A),8)+" "+HEX$(A)+STR$(A)
240 RETURN
250 REM End
260 DATA &h00,&h20,&h30
